{
  "name": "amiya",
  "description": "amiya React component",
  "version": "0.57.5",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "keywords": [
    "ant",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/viewweiwu/amiya.git"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "antd": "4.x",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/classnames": "^2.3.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "classnames": "^2.3.1",
    "dumi": "^1.1.38",
    "father-build": "^1.20.0",
    "gh-pages": "^3.0.0",
    "jsx-transpiler": "^0.1.4",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-helmet-async": "^1.1.2",
    "styled-components": "^5.3.3",
    "tslib": "^2.3.1",
    "yorkie": "^2.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%"
  ],
  "dependencies": {
    "amiya": "0.56.0"
  }
}
